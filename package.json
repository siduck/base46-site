	},
	"devDependencies": {
		"@iconify/json": "^2.2.288",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"unocss": "^0.65.2",
		"vite": "^5.4.11"
	}
}
